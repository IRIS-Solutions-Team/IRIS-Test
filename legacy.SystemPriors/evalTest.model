!transition_variables
    x, y
!transition_shocks
    ex, ey
!parameters
    a=0.8, b=0.2, c=0.7, d=0.2
!transition_equations
    x = a*x{-1} + b*y{-1} + ex;
    y = c*x{-1} + d*y{-1} + ey;
!measurement_variables
    xx, yy
!measurement_shocks
    exx, eyy
!measurement_equations
    xx = x + exx;
    yy = y + eyy;

