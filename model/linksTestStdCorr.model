!transition_variables
    x, y
!parameters
    rhox, mux, rhoy, muy
!transition_shocks
    epsx, epsy
!transition_equations
    x = rhox*x{-1} + (1-rhox)*mux + epsx !! x = mux;
    y = rhoy*y{-1} + (1-rhoy)*muy + epsy !! y = muy;
!measurement_variables
    xx, yy
!measurement_equations
    xx = x;
    yy = y;
!links
    std_epsx := std_epsy*2;
    corr_epsy__epsx := std_epsy*0.1;